{
  "name": "@keystone.sh/web",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "ks:dev": "ks env checkout default",
    "ks:prod": "ks env checkout production",
    "start": "react-static start &> keystone-web.start.log",
