    "purgecss-webpack-plugin": "^1.6.0",
    "react-hot-loader": "^4.12.18",
    "react-static-plugin-tailwindcss": "^0.1.3",
    "serve": "^11.1.0",
    "tailwindcss": "^1.1.4"
