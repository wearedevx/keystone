    "query-string": "^6.9.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-static": "^7.2.3",
    "react-static-plugin-reach-router": "^7.2.0",
    "react-static-plugin-sitemap": "^7.2.0",
    "react-static-plugin-source-filesystem": "^7.2.0",
    "zustand": "^2.2.1"
  },
  "devDependencies": {
    "@babel/runtime": "^7.7.6",
    "babel-eslint": "^10.0.2",
    "babel-plugin-universal-import": "^4.0.0",
