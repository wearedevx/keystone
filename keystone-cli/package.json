{
  "name": "@keystone.sh/cli",
  "description": "Decentralized workspaces for developers to store, share and use secrets securely.",
  "version": "0.0.36",
  "author": "wearedevx.com",
  "bin": {
    "ks": "./bin/run"
  },
  "bugs": "https://github.com/wearedevx/keystone/issues",
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://keystone.sh",
  "keywords": [
    "keystone dotenv env .env environment variables config settings secrets key blockstack decentralized blockchain"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "hooks": {
      "init": "./src/lib/hooks/spacify"
    },
    "bin": "ks",
    "plugins": [
      "@oclif/plugin-autocomplete",
      "@oclif/plugin-help"
    ]
  },
  "repository": "https://github.com/wearedevx/keystone",
  "scripts": {
    "update-readme":"oclif-dev readme",
    "postpack": "rm -f oclif.manifest.json && git add README.md && git commit -m 'Update CLI Readme'",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "export NODE_NO_WARNINGS=1 && jest --runInBand ./src/tests/index.spec.js",
    "lint": "eslint",
    "pack": "oclif-dev pack",
    "version": "oclif-dev readme && git add README.md",
    "build": ""
  },
  "jest": {
    "testEnvironment": "node"
  },
  "dependencies": {
    "@keystone.sh/core": "~0.0.20",
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/errors": "^1",
    "@oclif/plugin-autocomplete": "^0.1",
    "@oclif/plugin-help": "^2",
    "axios": "^0.19.0",
    "blockstack": "21.0.0-beta.1",
    "chalk": "^2.4.2",
    "cli-ux": "^5.3.0",
    "daffy": "^1.0.3",
    "dotenv": "^8.2.0",
    "elliptic": "^6.5.2",
    "filename-regex": "^2.0.1",
    "filter-console": "^0.1.1",
    "indent-string": "^4.0.0",
    "inquirer": "^6.4.1",
    "is-email": "^1.0.0",
    "listr": "^0.14.3",
    "lodash": "^4.17.15",
    "normalize-path": "^3.0.0",
    "open": "^6.4.0",
    "rewire": "^4.0.1",
    "rimraf": "^3.0.2",
    "strip-ansi": "^6.0.0",
    "treeify": "^1.1.0",
    "update-check": "^1.5.3",
    "uuid": "^3.3.3",
    "walkdir": "^0.4.1",
    "wrap-ansi": "^6.2.0"
  },
  "devDependencies": {
    "debug": "^4.1.1",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "17.1.1",
    "eslint-config-oclif": "^3.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "globby": "^11.0.0",
    "jest": "^24.9.0",
    "mock-stdin": "^0.3.1",
    "nock": "^11.9.1",
    "prettier": "^1.19.1",
    "user-home": "^2.0.0"
  }
}
